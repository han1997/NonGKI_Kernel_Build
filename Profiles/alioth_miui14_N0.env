CONFIG_ENV=Profiles/alioth_miui14_N0.env

DEVICE_NAME=redmi_k40_cn
DEVICE_CODENAME=alioth

CUSTOM_CMDS:CLANG_TRIPLE=aarch64-linux-gnu-
EXTRA_CMDS:LD=ld.lld LLVM=1 LLVM_IAS=1

KERNEL_SOURCE=https://github.com/EmanuelCN/kernel_xiaomi_sm8250.git
KERNEL_BRANCH=staging
CLANG_SOURCE=https://github.com/llvm/llvm-project/releases/download/llvmorg-19.1.7/LLVM-19.1.7-Linux-X64.tar.xz
CLANG_BRANCH=

GCC_GNU=false
GCC_64_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9
GCC_64_BRANCH=android12L-release
GCC_32_SOURCE=https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/x86/x86_64-linux-android-4.9
GCC_32_BRANCH=android12L-release

# DEFCONFIG_SOURCE=https://raw.githubusercontent.com/EmanuelCN/kernel_xiaomi_sm8250/refs/heads/staging/arch/arm64/configs/vendor/alioth_defconfig
DEFCONFIG_SOURCE=
DEFCONFIG_NAME=alioth_defconfig
DEFCONFIG_ORIGIN_DEFCONFIG=

KERNELSU_SOURCE=https://raw.githubusercontent.com/rifsxd/KernelSU-Next/next-susfs/kernel/setup.sh
KERNELSU_BRANCH=next-susfs
KERNELSU_NAME=KernelSU-Next

SUSFS_ENABLE=true
SUSFS_FIXED=false

AK3_SOURCE=https://github.com/osm0sis/AnyKernel3.git
AK3_BRANCH=master

BOOT_SOURCE=https://github.com/boot_author/android_kernel_codename/raw/refs/heads/main/rom/source_imgs/boot.img

LXC_ENABLE=false

HAVE_NO_DTBO=false
HAVE_NO_DTBO_TOOL=false

ROM_TEXT=rom_android
